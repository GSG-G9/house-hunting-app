{
    "env": {
      "commonjs": true,
      "es2021": true,
      "node": true
    },
    "extends": [
      "airbnb-base",
      "prettier"
    ],
    "parserOptions": {
      "ecmaVersion": 12
    },
       "rules": {
        "react/jsx-filename-extension":[
            1,
            {"extensions":[".js",".jsx"]}
        ],
        "no-nested-ternary": "off",
    "react/prefer-stateless-function": [0],
    "react/jsx-props-no-spreading": [0],
    "quotes": [
      2,
      "single",
      {
        "avoidEscape": true,
        "allowTemplateLiterals": true
      }
    ],
    "prettier/prettier": [
      "error",
      {
        "trailingComma": "es5",
        "singleQuote": true
      }
    ],
    "react/state-in-constructor": [0, "always"]
  },
  "plugins": ["react", "prettier"]
} 
    

