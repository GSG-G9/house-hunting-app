{
  "env": {
      "browser": true,
      "commonjs": true,
      "es2021": true,
      "node": true
  },
  "extends": [
      "airbnb-base",
      "plugin:prettier/recommended"
  ],
  "parserOptions": {
      "ecmaVersion": 12
  },

  "plugins": [
    "prettier"
  ],
  
  "rules": {
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "trailingComma": "all"
        }
      ]
    }
}